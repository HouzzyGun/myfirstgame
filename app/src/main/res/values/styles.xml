<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorOne</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">#3C3F41</item>
        <item name="android:textColor">@color/colorThree</item>

    </style>

    <style name="ConfigTheme" parent="Theme.AppCompat.Light">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorOne</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">#3C3F41</item>
        <item name="android:textColor">@color/colorThree</item>
        <item name="title">Configurações</item>

    </style>

    <style name="AlertTheme" parent="Theme.AppCompat.DayNight.Dialog.Alert">
        <!-- Customize your theme here. -->
        <item name="android:textColor">@color/colorFour</item>
        <item name="android:radius">20dp</item>

    </style>

</resources>